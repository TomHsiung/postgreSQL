# List schemas
\dn

# Show current schema
SELECT current_schema();

# Select a schema
SET search_path TO schemaname;

# Create a schema
CREATE SCHEMA myschema;

# Remove a schema
DROP SCHEMA schemaname;

# List databases
\l

# Select a database
\c databasename;

# Rename a database
ALTER DATABASE name RENAME TO new_name;

# List tables
\dt


# List users
\du
